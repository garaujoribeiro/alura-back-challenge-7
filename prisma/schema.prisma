// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Depoimento {
  id String @id @default(uuid())
  depoimento String
  autor String
  imagem String?
}

model Destino {
  id String @id @default(uuid())
  nome String
  preco Float
  imagem String?
}
